
<h2>RESET PASSWORD</h2>
<%= form_tag(password_reset_path,method: :put) do  %>
  
  
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  
  
    <%= label_tag :password_confirmation %>
    <%= password_field_tag :password_confirmation %>
  
  <%= submit_tag "Update Password"%>
<% end %>

